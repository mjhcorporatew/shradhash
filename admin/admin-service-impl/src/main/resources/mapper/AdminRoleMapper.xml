    <select id="selectByAdminId" parameterType="Integer" resultType="AdminRoleDO">
      SELECT
        ar.id, ar.admin_id, ar.role_id
      FROM admin a, admin_role ar
      WHERE a.id = #{adminId}
      AND a.id = ar.admin_id
